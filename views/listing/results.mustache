<div class="result-list">
{{#results}}
    <article class="listing preview">
        <aside>{{#mainImage}}<img src="{{mainImage}}" alt="" />{{/mainImage}}</aside>
        <header>
            <h1><a href="/listing/{{_id}}">{{name}}</a></h1>
        </header>
        <p>{{city}}, {{province}}</p>
    </article>
{{/results}}
</div>
<div class="sidebar">
    <div class="related">
        <div class="map" data-query="{{defaultLocation}}"></div>
        {{>search_refine_result}}
    </div>
</div>
<script type="text/javascript">
$(document.ready(function() {
    $('div.map').gvmap().auto();
});
