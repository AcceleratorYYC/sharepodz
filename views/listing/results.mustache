<div class="col main-content-wide">
    <h1>Results?</h1>
{{#results}}
    <article class="listing preview">
        <aside>{{#mainImage}}<a href="/listing/{{_id}}"><img src="{{mainImage}}" width="143" height="95" alt="" /></a>{{/mainImage}}</aside>
        <header>
            <h1><a href="/listing/{{_id}}">{{name}}</a></h1>
        </header>
        <p class="address">{{city}}, {{province}}</p>
    </article>
{{/results}}
</div>
<div class="sidebar">
    <div class="related">
        <div class="map" data-query="{{defaultLocation}}"></div>
        {{>search_refine_result}}
    </div>
</div>
<script type="text/javascript">
$(document.ready(function() {
    $('div.map').gvmap().auto();
});
</script>