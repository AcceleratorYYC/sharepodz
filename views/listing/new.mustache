<form action="/listing/create" method="post">
	Title: <input type="text" name="title"><br>
	Description: <input type="textarea" name="description"><br>
	City: <input type="text" name="city"><br>
	Province: <input type="text" name="province"><br>
	Address 1: <input type="text" name="address1"><br>
	Address 2: <input type="text" name="address2"><br>
	Postal Code: <input type="text" name="postalCode"><br>
	Phone Number: <input type="phone" name="phone"><br>
	Website: <input type="text" name="website"><br>
	Twitter: <input type="text" name="twitter"><br>
	Email: <input type="email" name="email"><br>
	Availability Start Date: <input type="date" name="startDate"><br>
    Availability End Date: <input type="date" name="endDate"><br>
	Tags: <input type="text" name="tags"><br>
	
	<input type="submit" value="Create">
</form>